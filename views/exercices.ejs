<% include partials/header %>

<div class='container'>
  <div class='formContainer'>
    <h1>Lista ćwiczeń: </h1>
    <div class="ui bulleted link list">
    <% result.forEach(function(exercise){ %>
      <li class='item'> <%= exercise["dtype"] %> </li>
    <% }) %> 
  </div>  
    <h2><%= info %></h2>

    <form action='/insrt' method='POST'>
      <input type='text' name='search' class="WrktIDopt" placeholder="Dodaj nowe ćwiczenie...">
      <input type="submit" class='wrktBttn' value="Zatwierdź">
    </form>

    <form action='/dlt' method='GET'>
      <input type='text' name='search' class="WrktIDopt" placeholder="Usuń ćwiczenie...">
      <input type="submit" class='wrktBttn' value="Zatwierdź">
    </form>
  </div>
</div>

<% include partials/footer %>
         



  