<% include partials/header %>

<h2><%= info %></h2>

<form action='/insrt' method='POST'>
  <p>Dodaj nowe ćwiczenie:</p>
  <input type='text' name='search' placeholder="Wpisz nazwę ćwiczenia...">
  <input type="submit" value="Zatwierdź">
</form>

<form action='/dlt' method='GET'>
  <p>Usuń ćwiczenie:</p>
  <input type='text' name='search' placeholder="Wpisz nazwę ćwiczenia...">
  <input type="submit" value="Zatwierdź">
</form>


<h1>Lista ćwiczeń: </h1>

<% result.forEach(function(exercise){ %>
  <li> <%= exercise["DTYPE"] %> </li>
  <% }) %> 
     

